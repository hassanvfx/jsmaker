{
  "name": "sunomaker",
  "version": "0.1.0",
  "description": "Adaptive AI Remixing System for Corrido Production",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:server\"",
    "dev:vite": "vite",
    "dev:server": "source venv/bin/activate && python server.py",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test:atomic": "NODE_OPTIONS=--experimental-vm-modules jest tests/unit",
    "test:api": "NODE_OPTIONS=--experimental-vm-modules jest tests/api",
    "test:integration": "RUN_INTEGRATION_TESTS=true NODE_OPTIONS=--experimental-vm-modules jest tests/integration"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^8.2.2",
    "dotenv": "^17.2.3",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "vite": "^5.4.0"
  }
}
